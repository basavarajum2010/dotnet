	<Style x:Key="LeftScrollButton" TargetType="{x:Type Button}">
	<Setter Property="SnapsToDevicePixels" Value="True"/>
		<Setter Property="OverridesDefaultStyle" Value="true"/>
		<Setter Property="Focusable" Value="false"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Button}">
				<ControlTemplate.Resources>
				<Storyboard x:Key="OverStoryboard">
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00" Value="0.45"/>
								<SplineDoubleKeyFrame KeySpline="0,0,0,1" KeyTime="00:00:00.5000000" Value="1"/>
							</DoubleAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Visibility)">
								<DiscreteObjectKeyFrame KeyTime="00:00:00" Value="{x:Static Visibility.Visible}"/>
							</ObjectAnimationUsingKeyFrames>
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
							<ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
								<SplineColorKeyFrame KeyTime="00:00:00.5000000" Value="White" KeySpline="0,0,0,1"/>
							</ColorAnimationUsingKeyFrames>
							
			   
						</Storyboard>
						<Storyboard x:Key="OutStoryboard">
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Visibility)">
								<DiscreteObjectKeyFrame KeyTime="00:00:00.5000000" Value="{x:Static Visibility.Collapsed}"/>
							</ObjectAnimationUsingKeyFrames>
							<ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
								<SplineColorKeyFrame KeyTime="00:00:00.5000000" Value="Transparent" KeySpline="0,0,0,1"/>
							</ColorAnimationUsingKeyFrames>

							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0.7" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
						
						</Storyboard>
						
						<Storyboard x:Key="PressedStoryboard">
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00" Value="0.45"/>
								<SplineDoubleKeyFrame KeySpline="0,0,0,1" KeyTime="00:00:00.5000000" Value="1"/>
							</DoubleAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Visibility)">
								<DiscreteObjectKeyFrame KeyTime="00:00:00" Value="{x:Static Visibility.Visible}"/>
							</ObjectAnimationUsingKeyFrames>
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
							<ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
								<SplineColorKeyFrame KeyTime="00:00:00.5000000" Value="White" KeySpline="0,0,0,1"/>
							</ColorAnimationUsingKeyFrames>
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="RootGrid" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Duration="00:00:00.0010000">
								<SplineDoubleKeyFrame KeyTime="00:00:00" Value="3"/>
							</DoubleAnimationUsingKeyFrames>
			   
						</Storyboard>
						<Storyboard x:Key="ReleaseStoryboard">
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="PressedIndicatorBorder" Storyboard.TargetProperty="(UIElement.Visibility)">
								<DiscreteObjectKeyFrame KeyTime="00:00:00.5000000" Value="{x:Static Visibility.Collapsed}"/>
							</ObjectAnimationUsingKeyFrames>
							<ColorAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
								<SplineColorKeyFrame KeyTime="00:00:00.5000000" Value="Transparent" KeySpline="0,0,0,1"/>
							</ColorAnimationUsingKeyFrames>

							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="Border" Storyboard.TargetProperty="(UIElement.Opacity)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0.7" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="RootGrid" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
								<SplineDoubleKeyFrame KeyTime="00:00:00.2000000" Value="0" KeySpline="0,0,0,1"/>
							</DoubleAnimationUsingKeyFrames>
						</Storyboard>
					</ControlTemplate.Resources>
					<Grid x:Name="RootGrid" Height="98" RenderTransformOrigin="0.5,0.5">
						<Grid.RenderTransform>
							<TransformGroup>
								<ScaleTransform/>
								<SkewTransform/>
								<RotateTransform/>
								<TranslateTransform/>
							</TransformGroup>
						</Grid.RenderTransform>
						<i:Interaction.Behaviors>
							<behav:TapBehavior/>
						</i:Interaction.Behaviors>
						<Border Background="Black" Margin="12,12"  Opacity="0.025" CornerRadius="8"/>
						<Border Background="Black" Margin="11,11" Opacity="0.05" CornerRadius="8"/>
					<Border 
							x:Name="BaseBorder"
							Margin="24,12" 
							CornerRadius="8"
							BorderBrush="{x:Null}"
							BorderThickness="2"  d:LayoutOverrides="Width, Height" Background="{DynamicResource Button_bg_Orange_Pressed}" />
						<Border 
							x:Name="PressedIndicatorBorder"
							Margin="12,12" 
							CornerRadius="8"
							BorderBrush="{x:Null}"
							BorderThickness="1" Background="White" Opacity="0" d:LayoutOverrides="Width, Height" Visibility="Collapsed">
							<Border.Effect>
								<BlurEffect Radius="18"/>
							</Border.Effect>
						</Border>
						
						<Image Source="Images/advancerightbutton.png" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center"/>
					<Border 
							x:Name="Border"
							Margin="12,12" 
							CornerRadius="8"
							BorderThickness="2"  d:LayoutOverrides="Width, Height" Opacity="0.7">
							<Border.BorderBrush>
								<SolidColorBrush/>
							</Border.BorderBrush>
						</Border>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
				  <Trigger.ExitActions>
								<BeginStoryboard x:Name="OutStoryboard_BeginStoryboard1"  Storyboard="{StaticResource OutStoryboard}"/>
								<StopStoryboard BeginStoryboardName="OverStoryboard_BeginStoryboard"/>
								<StopStoryboard BeginStoryboardName="PressedStoryboard_BeginStoryboard"/>
								<StopStoryboard BeginStoryboardName="ReleaseStoryboard_BeginStoryboard"/>
							</Trigger.ExitActions>
							<Trigger.EnterActions>
								<BeginStoryboard x:Name="OverStoryboard_BeginStoryboard"  Storyboard="{StaticResource OverStoryboard}"/>
								<StopStoryboard BeginStoryboardName="OutStoryboard_BeginStoryboard1"/>
								<StopStoryboard BeginStoryboardName="PressedStoryboard_BeginStoryboard"/>
								<StopStoryboard BeginStoryboardName="ReleaseStoryboard_BeginStoryboard"/>
							</Trigger.EnterActions>
						</Trigger>
						 <Trigger Property="IsPressed" Value="true">
							<Trigger.ExitActions>
								<BeginStoryboard x:Name="ReleaseStoryboard_BeginStoryboard" Storyboard="{StaticResource ReleaseStoryboard}"/>
							</Trigger.ExitActions>
							<Trigger.EnterActions>
								<BeginStoryboard x:Name="PressedStoryboard_BeginStoryboard" Storyboard="{StaticResource PressedStoryboard}"/>
							</Trigger.EnterActions>
						</Trigger>
							<Trigger Property="IsEnabled" Value="false">
								<Setter Property="Opacity" TargetName="RootGrid" Value="0.25"/>
							</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
