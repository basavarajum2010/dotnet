            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="PressIndicatorGrid" d:LayoutOverrides="Height" Opacity="0" Visibility="Collapsed">
                <Grid.Effect>
                    <BlurEffect Radius="14"/>
                </Grid.Effect>
                <Grid.Background>
                    <SolidColorBrush/>
                </Grid.Background>
                <TextBlock Text="{TemplateBinding Content}" TextWrapping="Wrap" d:LayoutOverrides="Width, Height" FontFamily="Segoe UI Light" FontSize="21.333" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="25,0,0,0"/>
                <Image HorizontalAlignment="Left" VerticalAlignment="Center" Width="Auto" Source="../../Resource/Images/CloseIcon.png" Stretch="None" RenderTransformOrigin="0.5,0.5">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
