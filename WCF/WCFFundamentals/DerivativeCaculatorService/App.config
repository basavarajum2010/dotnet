<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="DerivativeCaculatorService.DerivativeCaculatorServiceType" behaviorConfiguration="AllowHttpGetMetaData">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8000/Derivatives/"/>
            <add baseAddress="net.tcp://localhost:8000/Derivatives/"/>
          </baseAddresses>
        </host>
        <endpoint address="Caculator" binding="basicHttpBinding" contract="DerivativeCaculatorService.IDerivativeCaculator" ></endpoint>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="AllowHttpGetMetaData">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>